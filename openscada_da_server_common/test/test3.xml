<?xml version="1.0" encoding="UTF-8"?>
<hive:hive
  xmlns:common="http://openscada.org/Common"
  xmlns:hive="http://openscada.org/DA/Hive"
  xmlns:item="http://openscada.org/DA/Hive/DataItem"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://openscada.org/DA/Hive ../schema/Hive.xsd http://openscada.org/DA/Hive/DataItem DataItem.xsd http://openscada.org/Common Common.xsd ">

  <item-templates>
    <template id="template1" item-pattern=".*">

    	<item-attributes>
    		<attribute name="attr1">
    			<string>Hallo Welt</string>
    		</attribute>
    	</item-attributes>
    	
    	<browser-attributes>
    		<attribute name="attr1">
    			<string>Hallo Welt</string>
    		</attribute>
    	</browser-attributes>

    </template>
  </item-templates>
  
  <items>
    <item:data-item id="item1">
      <item-attributes>
        <attribute name="attr1">
          <string>Hello World</string>
        </attribute>
        <attribute name="attr2">
          <boolean>true</boolean>
        </attribute>
      </item-attributes>
    </item:data-item>
  </items>

  <browser>
  	<hive:folder></hive:folder>
  </browser>
  
</hive:hive>
