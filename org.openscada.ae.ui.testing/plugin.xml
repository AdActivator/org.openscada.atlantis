<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="org.openscada.ae.ui.testing.views.ConditionsView"
            id="org.openscada.ae.ui.testing.views.ConditionsView"
            name="Conditions"
            restorable="false">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.openscada.ae.ui.testing.oc1.BrowserEntryBean"
            objectClass="org.openscada.ae.ui.data.BrowserEntryBean">
         <action
               class="org.openscada.ae.ui.testing.actions.OpenConditions"
               enablesFor="1"
               id="org.openscada.ae.ui.testing.actions.OpenConditions"
               label="Open Conditions"
               menubarPath="additions">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.openscada.ae.ui.testing.oc2.ConditionStatusBean"
            objectClass="org.openscada.ae.ui.testing.views.ConditionStatusBean">
         <action
               class="org.openscada.ae.ui.testing.actions.AcknowledgeAction"
               enablesFor="1"
               id="org.openscada.ae.ui.testing.actions.Acknowledge"
               label="Acknowledge"
               menubarPath="additions">
         </action>
      </objectContribution>
   </extension>

</plugin>
