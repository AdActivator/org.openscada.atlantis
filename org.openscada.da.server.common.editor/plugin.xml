<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated Common -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.openscada.common.presentation.CommonModelWizardID"
            name="%_UI_CommonModelWizard_label"
            class="org.openscada.common.presentation.CommonModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CommonModelFile.gif">
         <description>%_UI_CommonModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated Common -->
      <editor
            id="org.openscada.common.presentation.CommonEditorID"
            name="%_UI_CommonEditor_label"
            icon="icons/full/obj16/CommonModelFile.gif"
            extensions="common"
            class="org.openscada.common.presentation.CommonEditor"
            contributorClass="org.openscada.common.presentation.CommonActionBarContributor">
      </editor>
   </extension>

</plugin>
